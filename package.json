    },
    "homepage": "https://github.com/TobiasNickel/trompt#readme",
    "repository": {
        "type": "git",
        "url": "git+https://github.com/TobiasNickel/trompt.git"
    },
    "bugs": {
        "url": "https://github.com/TobiasNickel/trompt/issues"
    },
    "devDependencies": {
        "@eslint/js": "^9.9.0",
        "@types/node": "^24.6.2",
        "eslint": "^8.57.0",
        "typescript": "^5.0.0"
    },
    "exports": {
        ".": {
            "import": "./index.js",
            "types": "./index.d.ts"
        }
    },
    "files": [
        "index.js",
        "index.d.ts",
        "README.md",
        "CHANGELOG.md",
        "LICENSE"
    ],
    "scripts": {
