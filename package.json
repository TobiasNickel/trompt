        "typescript",
        "inquirer",
        "questions"
    ],
    "main": "index.js",
    "types": "index.d.ts",
    "version": "1.0.1",
    "license": "MIT",
    "type": "module",
    "engines": {
        "node": ">=16.0.0"
    },
    "dependencies": {
